/* remove an element from a position in the array

*/
#include <iostream>
using namespace std;
int main() {
    int n;cin>>n;
    int arr[n],pos,elm;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
  cin>>pos;
  for(int i=pos;i<n-1;i++){
      arr[i]=arr[i+1];
  }
  for(int i=0;i<n-1;i++) cout<<arr[i]<<" ";

}
